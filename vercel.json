{
  "version": 2,
  "builds": [
    {
      "src": "streamlit.py",
      "use": "@vercel/streamlit"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "streamlit.py"
    }
  ],
  "env": {
    "STREAMLIT_SERVER_PORT": 8080,
    "STREAMLIT_SERVER_HEADLESS": true,
    "STREAMLIT_BROWSER_SERVER_ADDRESS": "0.0.0.0"
  }
}
